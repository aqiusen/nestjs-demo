    "version": "0.2.0",
    "configurations": [
        
      {
        "type": "node",
        "request": "launch",
        "name": "node debug",
        "cwd": "${workspaceRoot}",
        "runtimeExecutable": "npm",
        "timeout": 60000,
        "runtimeArgs": [
            "run",
            "start:debug"
        ],
        "port": 3000,
        "autoAttachChildProcesses":true,
        "stopOnEntry": false
      },
      {
        "type": "node",
        "request": "attach",
        "name": "debug attach",
        "port": 3000
      }
    ]
  }